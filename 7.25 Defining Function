#include <iostream>

// Define introduction() here:
void introduction(std::string first_name, std::string last_name){

  std::cout << last_name;
  std::cout << ", ";
  std::cout << first_name;
  std::cout << " ";
  std::cout << last_name;

}

int main() {
  
  introduction("Beyonce", "Knowles");
  
}
