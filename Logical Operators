// Learning about &&

#include <iostream>

int main() {
  
  int hunger = true;
  int anger = true;
  
  // Write the code below:
  if (hunger==true && anger==true) {
      std::cout << "Hangry\n";
  }
  
}

//learning about ||

#include <iostream>

int main() {
  
  int day = 6;
  
  // Write the code below:
  if (day == 6 || day ==7) {
    std::cout << "Weekend\n";
  }

}

//learning about !

#include <iostream>

int main() {
  
  bool logged_in = false;
  
  // Write the code below:
  if (!logged_in) {
    std::cout << "Try again\n";
  }
  
}

